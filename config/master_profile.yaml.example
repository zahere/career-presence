# SYNC CONFIGURATION
# How platforms should be kept in sync
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
sync:
  source_of_truth: "config/master_profile.yaml"

  platforms:
    resume:
      template: "resume/base/master.tex"
      auto_sync: true
      sections_to_sync:
        - "personal"
        - "summaries.resume"
        - "experience"
        - "education"
        - "skills"
        - "projects"

    linkedin:
      auto_sync: false  # Manual review required
      sections_to_sync:
        - "personal"
        - "summaries.linkedin"
        - "experience"
        - "education"
        - "skills.linkedin_skills"

    github:
      auto_sync: true
      sections_to_sync:
        - "personal"
        - "summaries.github"
        - "projects"

    website:
      auto_sync: true
      sections_to_sync:
        - "all"

