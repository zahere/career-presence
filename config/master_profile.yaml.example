    company: "Previous Company"
    role: "Previous Role"
    type: "employee"
    location: "City, Country"
    start_date: "2023-01"
    end_date: "2024-01"

    short_description: |
      What you did at this role.

    bullets:
      - text: "Achievement with metric"
        metrics: ["metric"]
        keywords: ["keyword"]

